{
    "name": "malangphp/singo",
    "description": "Silex-based feature rich rest application skeleton",
    "require": {
        "symfony/validator": "~2.6",
        "swiftmailer/swiftmailer": "~5.3",
        "silex/silex": "~2.0@dev",
        "monolog/monolog": "~1.13",
        "symfony/security": "~2.6",
        "cnam/security-jwt-service-provider": "~0.0.6.0",
        "jowy/tactician-service-provider": "~1.0.4",
        "jowy/silex-config-service-provider": "~1.1.1",
        "jowy/silex-pimple-aware-event-dispatcher": "~1.0.0",
        "stack/builder": "~1.0",
        "ddesrosiers/silex-annotation-provider": "dev-master",
        "symfony/console": "~2.6",
        "willdurand/stack-negotiation": "~1.0",
        "asm89/stack-cors": "0.2.*"
    },
    "require-dev": {
        "symfony/yaml": "~2.6",
        "dflydev/doctrine-orm-service-provider": "~2.0"
    },
    "suggest" : {
        "dflydev/doctrine-orm-service-provider" : "If you use doctrine ORM in your apps",
        "saxulum/saxulum-doctrine-mongodb-odm-provider": "If you use doctrine ODM in your apps",
        "symfony/yaml": "if you use yaml based config"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Prasetyo Wicaksono",
            "email": "pras@openmailbox.org"
        }
    ],
    "autoload" : {
      "psr-4" : {
          "Singo\\": "src/"
      }
    },
    "minimum-stability": "stable"
}
